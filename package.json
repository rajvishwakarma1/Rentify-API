{
  "name": "rentify-api",
  "version": "0.1.0",
  "description": "Rentify API - Node.js/Express with MongoDB",
  "main": "src/server.js",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon --exec node src/server.js",
    "test": "echo \"No tests yet\" && exit 0",
    "seed": "node src/utils/seedData.js all",
    "seed:cities": "node src/utils/seedData.js cities",
    "seed:users": "node src/utils/seedData.js users",
    "seed:properties": "node src/utils/seedData.js properties",
    "seed:clear": "node src/utils/seedData.js clear",
  "db:reset": "node src/utils/seedData.js clear && node src/utils/seedData.js all",
  "seed:bulk": "node src/utils/seedData.js bulk"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
  "redis": "^4.6.12",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "helmet": "^7.1.0",
    "joi": "^17.13.3",
    "mongoose": "^8.6.0",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.4"
  }
}
